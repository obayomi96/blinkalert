{
	"manifest_version": 2,
	"name": "Blink Alert",
	"description": "A reminder for a computer user to take the blinking exercise",
	"version": "1.0.0",
	"icons": {"128": "eyeIcon_128.png"},
	"browser_action": {
		"default_icon": "eyeIcon.png",
		"default_popup": "popup.html"
	},
    "permissions": ["activeTab", "alarms"],
    "background": {
        "scripts": ["main.js"],
        "persistent": false
    }
}